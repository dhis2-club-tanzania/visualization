    "webpack-merge": "^4.2.1",
    "webpack-strip-log-loader": "^0.1.5",
    "xml-loader": "^1.2.1"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
